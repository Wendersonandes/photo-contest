<div id="photographer" class="container">
	<div class="row panel">
		<div class="col-md-4 bg_blur ">
			<%= image_tag("placeholder.png", alt: "Placeholder-user", class: "img-responsive") %>
		</div>
        <div class="col-md-8  col-xs-12">
           <%= image_tag(@photographer.avatar, alt: "Placeholder-user", class: "img-thumbnail picture") %>
           <div class="photographer-infos">
                <h1><%= @photographer.name %></h1>
                <h4><%= @photographer.short_description%></h4>
                <span><%= @photographer.biography %></span>
           </div>
        </div>
    </div>   
    
