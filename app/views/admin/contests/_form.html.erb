<%= simple_form_for([:admin, @contest], html: { multipart: true }) do |f| %>
  <%= f.input :title %>  
  <%= f.input :description, as: :text %>  
  <%= f.input :image, as: :file %>
  <%= f.input :opening_enrollment, as: :datetime %> 
  <%= f.input :closing_enrollment, as: :datetime %>
  <%= f.input :opening, as: :datetime %> 
  <%= f.input :closing, as: :datetime %>
  <%= f.button :submit %> 
<% end %>
