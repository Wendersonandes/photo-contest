<%= simple_form_for([:admin, @contest], html: { multipart: true }) do |f| %>
  <%= f.input :title %>
  <%= f.input :description, as: :text%>
  <%= f.input :image, as: :file %>
  <%= f.input :opening_enrollment, :as => :datetime_picker %>
  <%= f.input :closing_enrollment, :as => :datetime_picker %>
  <%= f.input :opening, :as => :datetime_picker %>
  <%= f.input :closing, :as => :datetime_picker %>
  <%= f.button :submit, class: 'btn btn-success' %>
<% end %>
